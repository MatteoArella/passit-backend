  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf -rf dist tsconfig.tsbuildinfo libs.zip",
    "build": "yarn run clean && cd nodejs && npm install --prod && cd .. && zip -rq libs.zip nodejs && yarn run compile",
    "compile": "tsc -p tsconfig.json",
    "prepublishOnly": "yarn run build",
    "test": "mocha -c test/index.ts -r ts-node/register test/**/*.spec.ts --recursive",
    "start": "ts-node -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "@aws-cdk/aws-cognito": "^1.78.0",
    "@aws-cdk/aws-s3": "^1.78.0",
    "@aws-cdk/aws-s3-notifications": "^1.78.0",
    "@aws-cdk/core": "^1.78.0",
    "@aws-cdk/custom-resources": "^1.78.0",
    "@passit/core": "^1.0.0",
    "aws-lambda": "^1.0.6",
    "dotenv": "^8.2.0",
    "@dicebear/avatars": "^4.5.2",
    "@dicebear/avatars-jdenticon-sprites": "^4.5.2",
    "sharp": "^0.27.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.66",
    "@types/aws-sdk": "^2.7.0",
    "@types/dotenv": "^8.2.0",
    "@types/sharp": "^0.27.1",
    "@types/uuid": "^8.3.0",
    "recursive-copy-cli": "^1.0.14",
    "rimraf": "~3.0.2"
  },
  "peerDependencies": {
    "@aws-cdk/aws-iam": "^1.78.0",
