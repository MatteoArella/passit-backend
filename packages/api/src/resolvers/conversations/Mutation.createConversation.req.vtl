#set($conversation = "mutation CreateConversation {
  createConversation(input : {
    userId: ""$ctx.identity.sub"",
    tutorId: ""$context.arguments.input.tutorId""
  }) {
    id
    userId
    conversationId
    createdAt
    updatedAt
  }
}")
{
  "version": "2018-05-29",
  "method": "POST",
  "resourcePath": "/graphql",
  "params": {
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "query": "$util.escapeJavaScript($conversation)"
    }
  }
}