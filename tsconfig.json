{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["ES2018"],
    "strict": true,
    "alwaysStrict": true,
    "declaration": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "composite": true,
    "esModuleInterop": true,
    "types": [ "node" ],
    "baseUrl": "."
  }
}
